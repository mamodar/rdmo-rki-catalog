	<question dc:uri="https://rdmo.rki.de/terms/questions/data_publication_notice/data_publication_notice/information_data/anonymization_explanation">
		<uri_prefix>https://rdmo.rki.de/terms</uri_prefix>
		<key>anonymization_explanation</key>
		<path>data_publication_notice/data_publication_notice/information_data/anonymization_explanation</path>
		<dc:comment/>
		<attribute dc:uri="https://rdmo.rki.de/terms/domain/project/dataset/sensitive_data/personal_data/anonymization/explanation"/>
